{
  "name": "naive-dark-mode",
  "version": "0.2.9",
  "description": "Dark Mode for Vue3 and Naive UI.",
  "private": false,
  "author": "Tohrusky",
  "license": "MIT",
  "main": "./dist/naive-dark-mode.mjs",
  "module": "./dist/naive-dark-mode.mjs",
  "types": "./dist/naive-dark-mode.d.ts",
  "web-types": "web-types.json",
  "files": [
    "dist/*.mjs",
    "dist/*.d.ts",
    "dist/*.js",
    "dist/*.cjs",
    "README.md",
    "LICENSE",
    "web-types.json"
  ],
  "homepage": "https://github.com/Tohrusky/naive-dark-mode",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohrusky/naive-dark-mode"
  },
  "keywords": [
    "naive-ui",
    "dark-mode",
    "vue3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && vite build",
    "preview": "pnpm build && vite preview",
    "lint": "prettier --write . && eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck": "pnpm lint && vue-tsc --noEmit",
    "lib": "pnpm typecheck && vite build --mode lib"
  },
  "dependencies": {
    "naive-ui": "^2.38.2",
    "sass": "^1.77.2",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@babel/types": "^7.24.6",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-core": "^3.4.27",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.19"
  }
}
