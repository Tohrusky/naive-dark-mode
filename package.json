{
  "name": "naive-dark-mode",
  "version": "0.2.1",
  "description": "Dark Mode for Vue3 and Naive UI.",
  "private": false,
  "author": "Tohrusky",
  "license": "MIT",
  "main": "./dist/naive-dark-mode.mjs",
  "module": "./dist/naive-dark-mode.mjs",
  "types": "./dist/naive-dark-mode.d.ts",
  "web-types": "web-types.json",
  "files": [
    "dist/*.mjs",
    "dist/*.d.ts",
    "dist/*.js",
    "dist/*.cjs",
    "README.md",
    "LICENSE",
    "web-types.json"
  ],
  "homepage": "https://github.com/Tohrusky/naive-dark-mode",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohrusky/naive-dark-mode"
  },
  "keywords": [
    "naive-ui",
    "dark-mode",
    "vue3"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && vite build",
    "preview": "pnpm build && vite preview",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck": "pnpm lint && vue-tsc --noEmit",
    "lib": "pnpm typecheck && vite build --mode lib"
  },
  "dependencies": {
    "naive-ui": "^2.35.0",
    "sass": "^1.69.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-core": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.18"
  }
}
